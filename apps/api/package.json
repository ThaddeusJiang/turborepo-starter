{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
    "start": "node -r esbuild-register ./src/index.ts",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:ci-skip": "jest --ci --coverage"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csv-stringify": "^6.2.3",
    "dayjs": "^1.11.7",
    "express": "^4.17.1",
    "immer": "^9.0.17",
    "lodash": "^4.17.21",
    "mathjs": "^11.5.0",
    "morgan": "^1.10.0",
    "normalizr": "^3.6.2",
    "surrealdb.js": "^0.5.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.11",
    "esbuild": "^0.14.38",
    "esbuild-register": "^3.3.2",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "*",
    "jest": "^26.6.3",
    "jest-presets": "*",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.3",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  }
}
